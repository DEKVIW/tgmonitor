# Telegram API 配置
TELEGRAM_API_ID=YOUR_TELEGRAM_API_ID
TELEGRAM_API_HASH=YOUR_TELEGRAM_API_HASH

# 数据库配置
DATABASE_URL=postgresql://USERNAME:PASSWORD@HOST:PORT/DATABASE_NAME

# 新增异步 URL（仅改协议）
DATABASE_URL_ASYNC=postgresql+asyncpg://USERNAME:PASSWORD@HOST:PORT/DATABASE_NAME

# 默认频道配置（用逗号分隔多个频道，不需要包含 @ 符号）
DEFAULT_CHANNELS=CHANNEL1,CHANNEL2,CHANNEL3

# 日志级别
LOG_LEVEL=INFO

# Docker 环境标识
DOCKER_ENV=true 

# 登录认证密钥（用于生成和验证登录token）
SECRET_SALT=YOUR_SECRET_SALT
